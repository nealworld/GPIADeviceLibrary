        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button Content="Button" Height="23" HorizontalAlignment="Left" Margin="22,32,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" />
        <TextBox Text="{Binding DisplayString}" Height="287" HorizontalAlignment="Left" Margin="154,12,0,0" Name="textBox1" VerticalAlignment="Top" Width="325" />
        <Button Content="Button" Height="23" HorizontalAlignment="Left" Margin="22,90,0,0" Name="button2" VerticalAlignment="Top" Width="75" Click="button2_Click" />
    </Grid>
</Window>